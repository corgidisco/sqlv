{
  "name": "sqlv",
  "version": "1.0.0",
  "description": "Very simple SQL Migrator.",
  "keywords": [
    "migrator",
    "database",
    "typescript"
  ],
  "author": "Cris <corgidisco@gmail.com>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/corgidisco/sqlv.git"
  },
  "bin": {
    "sqlv": "dist/cli.js"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "dev": "ts-node src/cli.ts",
    "prepack": "npm run build",
    "build": "rimraf ./dist && tsc -p .",
    "lint": "tslint -c ./tslint.json -p tsconfig.json 'src/**/*.ts' --format stylish",
    "jest": "jest test",
    "test": "npm run build && npm run lint && npm run jest"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "./node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "[^/]*\\.(test|spec)\\.(j|t)sx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json"
    ]
  },
  "dependencies": {
    "async-db-adapter": "^0.3.1",
    "chalk": "^2.4.1",
    "inquirer": "^6.0.0",
    "js-yaml": "^3.12.0",
    "yargs": "^12.0.1"
  },
  "devDependencies": {
    "@types/chalk": "^2.2.0",
    "@types/jest": "^23.1.4",
    "@types/js-yaml": "^3.11.2",
    "@types/mysql": "^2.15.5",
    "@types/node": "^10.1.0",
    "@types/yargs": "^11.1.0",
    "jest": "^23.2.0",
    "mysql": "^2.15.0",
    "rimraf": "^2.6.2",
    "ts-jest": "^23.0.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "^2.9.2"
  }
}
